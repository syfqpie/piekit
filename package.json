{
  "name": "piekit",
  "description": "React components, freshly baked",
  "private": false,
  "version": "0.0.14",
  "type": "module",
  "license": "MIT",
  "homepage": "https://piekit.cendol.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/syfqpie/piekit"
  },
  "bugs": {
    "url": "https://github.com/syfqpie/piekit/issues"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "vite",
    "kit"
  ],
  "main": "dist/piekit.cjs.js",
  "module": "dist/piekit.es.js",
  "types": "dist/piekit.d.ts",
  "style": "dist/piekit.css",
  "exports": {
    ".": {
      "types": "./dist/piekit.d.ts",
      "style": "./dist/piekit.css",
      "import": "./dist/piekit.es.js",
      "require": "./dist/piekit.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate": "node scripts/generate.cjs",
    "bump:patch": "pnpm version patch -m \"build: %s\"",
    "bump:minor": "pnpm version minor -m \"build: %s\"",
    "bump:major": "pnpm version major -m \"build: %s\"",
    "storydev": "storybook dev -p 6006",
    "storydocs": "storybook dev --docs -p 6007",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@carbon/icons-react": "^11.55.0",
    "clsx": "^2.1.1",
    "motion": "^12.4.10"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@eslint/js": "^9.19.0",
    "@storybook/addon-essentials": "^8.6.4",
    "@storybook/addon-interactions": "^8.6.4",
    "@storybook/blocks": "^8.6.4",
    "@storybook/manager-api": "^8.6.12",
    "@storybook/react": "^8.6.4",
    "@storybook/react-vite": "^8.6.4",
    "@storybook/theming": "^8.6.12",
    "@tailwindcss/vite": "^4.0.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-storybook": "^0.11.4",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.34.7",
    "storybook": "^8.6.4",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": "^4.0.6"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended",
      "plugin:storybook/recommended"
    ]
  }
}
